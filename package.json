{
  "name": "helloworld",
  "displayName": "HelloWorld",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.106.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],

  "contributes": {
    "commands": [
      {
        "command": "helloworld.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "fet.askQuestion",
        "category": "fetquest",
        "title": "Fet Askquestion"
      },
      {
        "command": "fetquest.greetings",
        "category": "fetquest",
        "title": "Fet Greetings"
      },
      {
        "command": "fetquest.hellopanel",
        "category": "fetquest",
        "title": "Hello Panel"
      },
      {
        "command": "fetquest.gitDiffPath",
        "category": "fetquest",
        "title": "Folder Path"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\""
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.107.0",
    "concurrently": "^9.2.1",
    "rollup": "^4.54.0",
    "svelte": "^5.46.1",
    "svelte-check": "^4.3.5",
    "svelte-preprocess": "^6.0.3"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup-plugin-svelte": "^7.2.3",
    "rollup-plugin-terser": "^7.0.2"
  }
}
